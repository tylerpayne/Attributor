    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "HotPotQA-SmolLM-135M-Instruct-float16",
            "type": "debugpy",
            "request": "launch",
            "program": "hotpot_qa.py",
            "console": "integratedTerminal",
            "args": "--model HuggingFaceTB/SmolLM-135M-Instruct --dtype float16 --device_map mps "
        }
    ]
}